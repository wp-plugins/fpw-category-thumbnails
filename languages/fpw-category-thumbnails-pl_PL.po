#: fpw-category-thumbnails.php:44
msgid "FPW Category Thumbnails"
msgstr "FPW Miniaturki Kategorii"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:56
msgid "Settings"
msgstr "Ustawienia"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:180
msgid "<strong>WARNING</strong>: Your theme has no support for <em>post thumbnails</em>!"
msgstr "<strong>OSTRZEŻENIE</strong>: Twój motyw nie ma wsparcia dla <em>miniaturek wpisów</em>!"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:181
msgid "You can continue with <em>Settings</em> but until you add <code>add_theme_support( 'post-thumbnails' );</code> to the theme's functions.php you will not be able to display thumbnails."
msgstr "Możesz kontynuować <em>Ustawienia</em>, jednakże do czasu wprowadzenia <code>add_theme_support( 'post-thumbnails' );</code> do functions.php motywu, wyświetlanie miniaturek nie będzie możliwe."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:187
msgid "Settings updated successfully."
msgstr "Aktualizacja ustawień zakończyła się pomyślnie."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:189
msgid "No changes detected. Nothing to update."
msgstr "Nie wykryto żadnych zmian. Nie ma nic do aktualizacji."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:193
msgid "Description"
msgstr "Opis"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:194
msgid "This plugin inserts a thumbnail based on category / thumbnail mapping while post / page is being created or updated."
msgstr "Ta wtyczka wstawia miniaturkę, na podstawie relacji kategoria / miniaturka, podczas tworzenia lub aktualizacji wpisu / strony."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:195
msgid "Note"
msgstr "Uwaga"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:195
msgid "please remember that your theme must support post thumbnails."
msgstr "proszę pamiętać, że Twój motyw musi wspomagać miniaturki wpisów."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:198
msgid "Instructions"
msgstr "Instrukcje"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:199
msgid "Enter <strong>IDs</strong> of thumbnail images for corresponding categories."
msgstr "Wprowadź <strong>identyfikatory miniaturek</strong> dla odpowiadających im kategorii."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:200
msgid "Enter <strong>0</strong> for categories without assignment."
msgstr "Wprowadź <strong>0</strong> dla kategorii bez przydziału."

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:216
msgid "Do not overwrite if post/page has thumbnail assigned allready"
msgstr "Nie dokonuj zmiany, jeśli wpis/strona ma już przydzieloną miniaturkę"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:221
msgid "Remove plugin's data from database on uninstall"
msgstr "Podczas usuwania wtyczki, usuń jej informacje z bazy danych"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:226
msgid "Category (ID)"
msgstr "Kategoria (ID)"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:227
msgid "Image ID"
msgstr "ID Obrazka"

# @ fpw-category-thumbnails
#: fpw-category-thumbnails.php:255
msgid "Update"
msgstr "Aktualizuj"

